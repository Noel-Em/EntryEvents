{
  "name": "entry-events",
  "version": "1.0.5",
  "type": "module",
  "description": "An event-driven system in TypeScript and JavaScript for handling custom events and listeners, designed to simplify event handling",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "event",
    "emit",
    "typescript",
    "javascript",
    "custom events",
    "listener",
    "event-driven",
    "node",
    "bun"
  ],
  "scripts": {
    "test": "node --import tsx ./example/index.ts",
    "b-test": "bun run ./example/index.ts",
    "n-test": "node --import tsx ./example/index.ts",
    "build": "tsup"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3"
  },
  "author": "Leonardo Ulino [NoelEm]",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Noel-Em/EntryEvents.git"
  },
  "bugs": {
    "url": "https://github.com/Noel-Em/EntryEvents/issues"
  },
  "homepage": "https://github.com/Noel-Em/EntryEvents#readme"
}
